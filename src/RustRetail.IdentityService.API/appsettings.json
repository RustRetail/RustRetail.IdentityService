{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Jwt": {
    "Issuer": "RustRetail.IdentityService",
    "Audience": "RustRetail",
    "SecretKey": "SecretKey",
    // 24 hours (1 Day)
    "AccessTokenExpiryInMilliseconds": 86400000,
    // 7 days (1 Week)
    "RefreshTokenExpiryInMilliseconds": 604800000
  }
}
